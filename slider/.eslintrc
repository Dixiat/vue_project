{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "rules": {
        "indent": [ 1, 4 ],
        "quotes": [ 2, "single" ],
        "linebreak-style": [ 1, "windows" ],
        "semi": [ 1, "always" ],
        "no-unused-vars": [1, { "vars": "all", "args": "after-used" }]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "window": true,
    },
    "extends": "eslint:recommended","vue"
}
